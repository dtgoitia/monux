{
  "name": "Monux",
  "version": "0.9.3",
  "description": "Unoffical Monzo Client for Desktop",
  "private": true,
  "main": "src/app.ts",
  "repository": "https://github.com/robjtede/monux",
  "author": "robjtede",
  "license": "MIT",
  "scripts": {
    "start": "webpack --watch",
    "ng": "ng",
    "lint": "ng lint",
    "webpack": "webpack",
    "build": "webpack --display-error-details && npm run build:electron",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "build:electron:tsc": "tsc ./src/app.ts --outDir ./dist --target es2015 --module commonjs --skipLibCheck",
    "build:electron": "npm run build:electron:tsc",
    "electron:serve": "npm run build:electron && npm run electron:test -- --serve",
    "electron:test": "electron ./dist/app.js",
    "electron:dev": "npm run build && npm run electron:test",
    "electron:prod": "npm run build:prod && npm run electron:test",
    "electron:linux": "npm run build:prod && node tools/package.js --platform=linux --arch=x64",
    "electron:windows": "npm run build:prod && node tools/package.js --platform=win32 --arch=ia32",
    "electron:mac": "npm run build:prod && node tools/package.js --platform=darwin --arch=x64",
    "test": "karma start ./karma.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet && npm run build",
    "e2e": "protractor ./protractor.conf.js",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@angular-redux/store": "^7.1.1",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "angular2-autosize": "^1.0.2",
    "common-tags": "^1.8.0",
    "core-js": "^2.5.7",
    "d3": "^5.4.0",
    "date-fns": "^2.0.0-alpha.1",
    "debug": "^3.1.0",
    "dexie": "^2.0.4",
    "electron": "^2.0.2",
    "electron-contextmenu-middleware": "^1.0.3",
    "electron-devtools-installer": "^2.2.4",
    "electron-image-menu": "^2.0.0",
    "electron-window-state": "^4.1.1",
    "img-exif": "^0.2.0",
    "keytar": "^4.2.1",
    "lodash": "^4.17.10",
    "ng2-img-tools": "^1.1.3",
    "p-iteration": "^1.1.7",
    "redux": "^4.0.0",
    "redux-actions": "^2.4.0",
    "redux-promise-middleware": "^5.1.1",
    "reflect-metadata": "^0.1.12",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "rxjs": "^6.2.0",
    "rxjs-compat": "^6.2.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "^6.0.5",
    "@angular/compiler-cli": "^6.0.3",
    "@ngtools/webpack": "^6.0.5",
    "@types/common-tags": "^1.4.0",
    "@types/d3": "^5.0.0",
    "@types/debug": "0.0.30",
    "@types/electron-config": "^0.2.1",
    "@types/electron-devtools-installer": "^2.0.3",
    "@types/electron-window-state": "^2.0.31",
    "@types/lodash": "^4.14.109",
    "@types/node": "^10.1.3",
    "@types/redux-actions": "^2.3.0",
    "@types/redux-promise": "^0.5.28",
    "@types/request-promise-native": "^1.0.14",
    "copy-webpack-plugin": "^4.5.1",
    "copyfiles": "^2.0.0",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "devtron": "^1.4.0",
    "eclint": "^2.7.0",
    "electron-packager": "^12.1.0",
    "electron-reload": "^1.2.2",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "json-types": "^1.0.0",
    "lint-staged": "^7.1.2",
    "mini-css-extract-plugin": "^0.4.0",
    "postcss": "^6.0.22",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.5",
    "prettier": "^1.13.2",
    "style-loader": "^0.21.0",
    "to-string-loader": "^1.1.5",
    "tslib": "^1.9.1",
    "tslint": "^5.10.0",
    "typescript": "^2.7.2",
    "webpack": "^4.10.1",
    "webpack-cli": "^2.1.4"
  }
}
