{
  "name": "monux",
  "productName": "Monux",
  "version": "0.9.3",
  "description": "Secure desktop client for managing your Monzo current account",
  "private": true,
  "main": "./dist/app.js",
  "repository": "https://github.com/robjtede/monux",
  "author": "robjtede",
  "license": "MIT",
  "scripts": {
    "start": "npm run build:dev:webpack -- --watch",
    "electron:dev": "electron ./dist/app.js",
    "electron:serve": "npm run build:electron && npm run electron:dev",
    "build:dev": "npm run build:dev:webpack && npm run build:electron",
    "build:dev:webpack": "webpack --mode development",
    "build": "cross-env NODE_ENV=production npm run build:webpack && npm run build:electron",
    "build:webpack": "webpack --mode production",
    "build:electron": "tsc ./src/app.ts --outDir ./dist --target es2015 --module commonjs --skipLibCheck",
    "build:mac": "npm run build && electron-builder --mac",
    "build:windows": "npm run build && electron-builder --win",
    "build:linux": "npm run build && electron-builder --linux",
    "build:all": "npm run build && electron-builder -mw",
    "precommit": "lint-staged",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@angular/common": "^6.1.0-beta.1",
    "@angular/compiler": "^6.1.0-beta.1",
    "@angular/core": "^6.1.0-beta.1",
    "@angular/platform-browser": "^6.1.0-beta.1",
    "@angular/platform-browser-dynamic": "^6.1.0-beta.1",
    "@angular/router": "^6.1.0-beta.1",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@ngrx/store-devtools": "^6.0.1",
    "common-tags": "^1.8.0",
    "core-js": "^2.5.7",
    "d3": "^5.5.0",
    "date-fns": "^2.0.0-alpha.1",
    "debug": "^3.1.0",
    "dexie": "^2.0.4",
    "electron-contextmenu-middleware": "^1.0.3",
    "electron-image-menu": "^2.0.0",
    "electron-window-state": "^4.1.1",
    "keytar": "^4.2.1",
    "lodash": "^4.17.10",
    "ng2-img-tools": "^1.1.3",
    "p-iteration": "^1.1.7",
    "reflect-metadata": "^0.1.12",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "rxjs": "^6.2.1",
    "rxjs-compat": "^6.2.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "^6.1.0-beta.1",
    "@angular/compiler-cli": "^6.1.0-beta.1",
    "@ngtools/webpack": "^6.1.0-beta.1",
    "@types/common-tags": "^1.4.0",
    "@types/d3": "^5.0.0",
    "@types/debug": "0.0.30",
    "@types/electron-config": "^0.2.1",
    "@types/electron-devtools-installer": "^2.0.3",
    "@types/electron-window-state": "^2.0.31",
    "@types/lodash": "^4.14.110",
    "@types/node": "^10.3.4",
    "@types/request-promise-native": "^1.0.15",
    "copy-webpack-plugin": "^4.5.1",
    "copyfiles": "^2.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "devtron": "^1.4.0",
    "eclint": "^2.7.0",
    "electron": "^2.0.2",
    "electron-builder": "^20.15.3",
    "electron-devtools-installer": "^2.2.4",
    "electron-reload": "^1.2.2",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "json-types": "^1.0.0",
    "lint-staged": "^7.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "postcss": "^6.0.22",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.5",
    "prettier": "^1.13.5",
    "style-loader": "^0.21.0",
    "to-string-loader": "^1.1.5",
    "tslib": "^1.9.2",
    "tslint": "^5.10.0",
    "typescript": "^2.8.4",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8"
  }
}
